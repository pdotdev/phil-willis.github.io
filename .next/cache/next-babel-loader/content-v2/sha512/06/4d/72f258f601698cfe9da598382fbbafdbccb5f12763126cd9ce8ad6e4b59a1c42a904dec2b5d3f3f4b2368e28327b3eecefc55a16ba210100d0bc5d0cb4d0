{"ast":null,"code":"import renderToString from \"next-mdx-remote/render-to-string\";\nimport hydrate from \"next-mdx-remote/hydrate\";\nimport { Hello, Test } from \"./components\";\nconst components = {\n  Test,\n  Hello\n};\n/**\n * serialize mdx to string\n */\n\nexport async function mdxToString(mdx) {\n  return await renderToString(mdx, {\n    components\n  });\n}\n/**\n * render string back to react components\n */\n\nexport function stringToMdx(string) {\n  return hydrate(string, {\n    components\n  });\n}","map":null,"metadata":{},"sourceType":"module"}